{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Server",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "yarn",
			"cwd": "${workspaceFolder}/server",
			"runtimeArgs": ["start"],
			"port": 9229,
			"autoAttachChildProcesses": true
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to process",
			"processId": "${command:PickProcess}",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
